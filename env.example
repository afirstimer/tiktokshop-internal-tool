# ============================================
# SUPABASE CONFIGURATION
# ============================================
# Lấy từ: https://app.supabase.com/project/YOUR_PROJECT/settings/api
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
# Service Role Key - CHỈ dùng cho batch script (có quyền cao hơn)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# ============================================
# REACT APP ENVIRONMENT VARIABLES
# ============================================
# Vite sử dụng prefix VITE_ cho env variables
# Copy từ SUPABASE_URL và SUPABASE_ANON_KEY ở trên
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# ============================================
# CLOUDINARY CONFIGURATION
# ============================================
# Lấy từ: https://console.cloudinary.com/settings/api-keys
CLOUDINARY_CLOUD_NAME=your-cloud-name
CLOUDINARY_API_KEY=your-api-key
CLOUDINARY_API_SECRET=your-api-secret

# ============================================
# FILE PATHS (Batch Upload Script)
# ============================================
# Thư mục chứa file cần upload (Windows path)
UPLOAD_DIR=./uploads
# Thư mục lưu file đã xử lý
PROCESSED_DIR=./processed
# Thư mục lưu file lỗi (nếu có)
ERROR_DIR=./errors

# ============================================
# APP CONFIGURATION
# ============================================
# Môi trường: development, production
NODE_ENV=development
# Port cho React app (nếu cần)
PORT=3000

